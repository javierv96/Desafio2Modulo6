<div class="row container m-auto my-4">
    {{#each productos}}
    <div class="col-4 producto my-3" data-nombre="{{nombre}}">
        <h3>{{nombre}}</h3>
        <img src="/{{imagen}}" alt="{{nombre}}">
        <h5 class="text-muted">En carro: {{seleccionado}}</h5>
    </div>
    {{/each}}
</div>

